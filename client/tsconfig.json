{
	"include": ["**/*", "../src/Types.ts", "../src/Types.ts", "../src/SetInfos.ts", "../src/parseCSV.ts"],
	"compilerOptions": {
		"allowJs": false,
		"baseUrl": ".",
		"module": "ESNext",
		"moduleResolution": "node",
		"lib": ["es2022", "dom", "dom.iterable"],
		"paths": {
			"@/*": ["src/*", "../dist/types/src/*"]
		},
		"skipLibCheck": false,
		"sourceMap": true,
		"strict": true,
		"target": "esnext",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"noImplicitReturns": true
	},
	"vueCompilerOptions": {
		"target": 3
	}
}
